<%- include('./navbar2.html') %>
  <!DOCTYPE html>
  <html lang="en">

  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Profile Page Design Example</title>

    <meta name="author" content="Codeconvey" />
    <link href="https://fonts.googleapis.com/css?family=Lato:300,400,700,900&display=swap" rel="stylesheet">
    <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.min.css'>
    <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.12.1/css/all.min.css'>

    <!--Only for demo purpose - no need to add.-->
    <link rel="stylesheet" href="studentData/css/demo.css" />

    <link rel="stylesheet" href="studentData/css/style.css" />
  </head>

  <body>

    <div class="ScriptTop">
      <div class="rt-container">
        <div class="col-rt-4" id="float-right">

          <!-- Ad Here -->

        </div>

      </div>
    </div>


    <header class="ScriptHeader">
      <div class="rt-container">
        <div class="col-rt-12">
          <div class="rt-heading">
            <h1>Student Profile Page </h1>
            <p>student profile page design.</p>
          </div>
        </div>
      </div>
    </header>

    <section>
      <div class="rt-container">
        <div class="col-rt-12">
          <div class="Scriptcontent">
            <%result.forEach(element=> { %>
              <!-- Student Profile -->
              <div class="student-profile py-4">
                <div class="container">
                  <div class="row">
                    <div class="col-lg-4">
                      <div class="card shadow-sm">
                        <div class="card-header bg-transparent text-center">
                          <img class="profile_img" src="https://source.unsplash.com/600x300/?student" alt="student dp">
                          <h3>
                            <%=element.fname %>
                              <%=element.lname %>
                          </h3>
                        </div>
                        <div class="card-body">
                          <p class="mb-0"><strong class="pr-1">Student ID:</strong>
                            <%=element.studentId%>
                          </p>
                          <p class="mb-0"><strong class="pr-1">Class:</strong>
                            <%=element.class%>
                          </p>
                          <p class="mb-0"><strong class="pr-1">Section:</strong>
                            <%=element.section%>
                          </p>
                        </div>
                      </div>
                    </div>
                    <div class="col-lg-8">
                      <div class="card shadow-sm">
                        <div class="card-header bg-transparent border-0">
                          <h3 class="mb-0"><i class="far fa-clone pr-1"></i>General Information</h3>
                        </div>
                        <div class="card-body pt-0">
                          <table class="table table-bordered">
                            <tr>
                              <th width="30%">FirstName</th>
                              <td width="2%">:</td>
                              <td>
                                <%=element.fname%>
                              </td>
                            </tr>
                            <tr>
                              <th width="30%">Last Name</th>
                              <td width="2%">:</td>
                              <td>
                                <%=element.lname%>
                              </td>
                            </tr>
                            <tr>
                              <th width="30%">Email</th>
                              <td width="2%">:</td>
                              <td>
                                <%=element.email%>
                              </td>
                            </tr>
                            <tr>
                              <th width="30%">Gender</th>
                              <td width="2%">:</td>
                              <td>
                                <%=element.gender%>
                              </td>
                            </tr>
                            <tr>
                              <th width="30%">Source of income(Father)</th>
                              <td width="2%">:</td>
                              <td>
                                <%=element.incomeSource%>
                              </td>
                            </tr>
                            <tr>
                              <th width="30%">Income</th>
                              <td width="2%">:</td>
                              <td>
                                <%=element.value%>
                              </td>
                            </tr>
                            <tr>
                              <th width="30%">Age</th>
                              <td width="2%">:</td>
                              <td>
                                <%=element.age%>
                              </td>
                            </tr>
                          </table>

                        </div>
                      </div>
                      <div style="height: 26px"></div>
                      <div class="card shadow-sm">
                        <div class="card-header bg-transparent border-0">
                          <h3 class="mb-0"><i class="far fa-clone pr-1"></i>Other Information</h3>
                        </div>
                        <p>
                          <%=element.bio %>
                        </p>
                        <div class="card-body pt-0">

                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <!-- partial -->

              </div>
          </div>
        </div>
        <% });%>
    </section>



    <!-- Analytics -->

  </body>

  </html>